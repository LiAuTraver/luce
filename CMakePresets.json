{
  "version": 3,
  "cmakeMinimumRequired": {
    "major": 3,
    "minor": 15,
    "patch": 0
  },
  "configurePresets": [
    {
      "name": "common",
      "hidden": true,
      "description": "Common options for all configurations. Note: please set the VCPKG_ROOT environment variable to the path of your vcpkg installation.",
      "generator": "Ninja",
      "binaryDir": "${sourceDir}/build/${presetName}",
      "toolchainFile": "$env{VCPKG_ROOT}/scripts/buildsystems/vcpkg.cmake",
      "cacheVariables": {
        "CMAKE_EXPORT_COMPILE_COMMANDS": "ON",
        "CMAKE_SUPPRESS_DEVELOPER_WARNINGS": "TRUE",
        "CMAKE_CXX_EXTENSIONS": "ON",
        "CMAKE_CXX_STANDARD": "23",
        "CMAKE_C_STANDARD": "23",
        "BUILD_SHARED_LIBS": "ON",
        "VCPKG_MANIFEST_INSTALL": "OFF",
        "VCPKG_MANIFEST_MODE": "OFF",
        "CMAKE_PREFIX_PATH": "$env{VCPKG_ROOT}/installed/$env{VCPKG_DEFAULT_TRIPLET}"
      }
    },
    {
      "name": "GNU",
      "hidden": true,
      "inherits": [
        "common"
      ],
      "cacheVariables": {
        "CMAKE_CXX_COMPILER": "g++",
        "CMAKE_C_COMPILER": "gcc"
      },
      "environment": {
        "CC": "gcc",
        "CXX": "g++"
      }
    },
    {
      "name": "Clang",
      "hidden": true,
      "inherits": [
        "common"
      ],
      "condition": {
        "type": "equals",
        "lhs": "${hostSystemName}",
        "rhs": "Windows"
      },
      "cacheVariables": {
        "CMAKE_C_COMPILER": "C:/Program Files/LLVM/bin/clang.exe",
        "CMAKE_CXX_COMPILER": "C:/Program Files/LLVM/bin/clang++.exe"
      },
      "environment": {
        "CC": "C:/Program Files/LLVM/bin/clang.exe",
        "CXX": "C:/Program Files/LLVM/bin/clang++.exe"
      }
    },
    {
      "name": "Clang-Linux",
      "hidden": true,
      "binaryDir": "${sourceDir}/build/${hostSystemName}-${presetName}",
      "inherits": [
        "common"
      ],
      "condition": {
        "type": "equals",
        "lhs": "${hostSystemName}",
        "rhs": "Linux"
      },
      "cacheVariables": {
        "CMAKE_C_COMPILER": "clang",
        "CMAKE_CXX_COMPILER": "clang++"
      },
      "environment": {
        "CC": "clang",
        "CXX": "clang++"
      }
    },
    {
      "name": "MSVC",
      "hidden": true,
      "inherits": [
        "common"
      ],
      "condition": {
        "type": "equals",
        "lhs": "${hostSystemName}",
        "rhs": "Windows"
      },
      "cacheVariables": {
        "CMAKE_CXX_COMPILER": "cl",
        "CMAKE_C_COMPILER": "cl",
        "CMAKE_CXX_FLAGS": "/permissive- /volatile:iso /Zc:inline /Zc:preprocessor /Zc:enumTypes /Zc:lambda /Zc:__cplusplus /Zc:externConstexpr /Zc:throwingNew /EHsc",
        "CMAKE_EXE_LINKER_FLAGS": "/MACHINE:X64"
      },
      "environment": {
        "CC": "cl",
        "CXX": "cl"
      }
    },
    {
      "name": "ClangCL",
      "hidden": true,
      "inherits": [
        "common"
      ],
      "description": "clang compiler, but with MSVC-compatible flags",
      "condition": {
        "type": "equals",
        "lhs": "${hostSystemName}",
        "rhs": "Windows"
      },
      "cacheVariables": {
        "CMAKE_CXX_COMPILER": "C:/Program Files/LLVM/bin/clang-cl.exe",
        "CMAKE_C_COMPILER": "C:/Program Files/LLVM/bin/clang-cl.exe",
        "CMAKE_CXX_FLAGS": "/EHsc"
      },
      "environment": {
        "CC": "C:/Program Files/LLVM/bin/clang-cl.exe",
        "CXX": "C:/Program Files/LLVM/bin/clang-cl.exe"
      }
    },
    {
      "name": "ClangCL-Debug",
      "hidden": false,
      "inherits": [
        "ClangCL"
      ],
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Debug"
      }
    },
    {
      "name": "ClangCL-Release",
      "hidden": false,
      "inherits": [
        "ClangCL"
      ],
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Release"
      }
    },
    {
      "name": "MSVC-Debug",
      "hidden": false,
      "inherits": [
        "MSVC"
      ],
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Debug"
      }
    },
    {
      "name": "MSVC-Release",
      "hidden": false,
      "inherits": [
        "MSVC"
      ],
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Release"
      }
    },
    {
      "name": "Clang-Debug",
      "hidden": false,
      "inherits": [
        "Clang"
      ],
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Debug"
      }
    },
    {
      "name": "Clang-Release",
      "hidden": false,
      "inherits": [
        "Clang"
      ],
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Release"
      }
    },
    {
      "name": "Clang-Linux-Debug",
      "hidden": false,
      "inherits": [
        "Clang-Linux"
      ],
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Debug"
      }
    },
    {
      "name": "Clang-Linux-Release",
      "hidden": false,
      "inherits": [
        "Clang-Linux"
      ],
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Release"
      }
    },
    {
      "name": "MSVC-Visual-Studio-Debug",
      "hidden": false,
      "inherits": [
        "MSVC"
      ],
      "architecture": "x64",
      "generator": "Visual Studio 17 2022",
      "cacheVariables": {
        "CMAKE_GENERATOR": "Visual Studio 17 2022",
        "CMAKE_BUILD_TYPE": "Debug"
      }
    },
    {
      "name": "MSVC-Visual-Studio-Release",
      "hidden": false,
      "inherits": [
        "MSVC"
      ],
      "architecture": "x64",
      "generator": "Visual Studio 17 2022",
      "cacheVariables": {
        "CMAKE_GENERATOR": "Visual Studio 17 2022",
        "CMAKE_BUILD_TYPE": "Release"
      }
    },
    {
      "name": "Qt6",
      "hidden": true,
      "cacheVariables": {
        "CMAKE_AUTOMOC": "ON",
        "CMAKE_AUTORCC": "ON",
        "CMAKE_AUTOUIC": "ON",
        "QT_VERSION_MAJOR": "6",
        "CMAKE_PREFIX_PATH": "$env{Qt6_DIR}/msvc2022_64/lib/cmake/Qt6"
      }
    },
    {
      "name": "Qt6-GNU-Debug",
      "description": "Currently only configuerd for Linux",
      "hidden": false,
      "inherits": [
        "Qt6",
        "GNU"
      ],
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Debug"
      }
    },
    {
      "name": "Qt6-GNU-Release",
      "description": "Currently only configuerd for Linux",
      "hidden": false,
      "inherits": [
        "Qt6",
        "GNU"
      ],
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Release"
      }
    },
    {
      "name": "Qt6-MSVC-Debug",
      "hidden": false,
      "inherits": [
        "Qt6",
        "MSVC"
      ],
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Debug"
      }
    },
    {
      "name": "Qt6-MSVC-Release",
      "hidden": false,
      "inherits": [
        "Qt6",
        "MSVC"
      ],
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Release"
      }
    },
    {
      "name": "CUDA",
      "hidden": true,
      "architecture": "x64",
      "inherits": [
        "MSVC"
      ],
      "cacheVariables": {
        "CMAKE_CUDA_COMPILER": "nvcc",
        "CMAKE_CUDA_STANDARD": "20",
        "CMAKE_CUDA_FLAGS": "-std=c++20",
        "CMAKE_CXX_STANDARD": "20",
        "CMAKE_C_STANDARD": "17",
        "CMAKE_CXX_COMPILER": "cl",
        "CMAKE_CXX_FLAGS": "/std:c++20  /Zc:preprocessor",
        "CUDA_TOOLKIT_ROOT_DIR": "C:/Program Files/NVIDIA GPU Computing Toolkit/CUDA/v12.6",
        "CUSPARSELT_ROOT": "C:/Program Files/NVIDIA cuSPARSELt/v0.6",
        "CMAKE_PREFIX_PATH": "C:/Users/LiAu/miniconda3/Lib/site-packages/torch/share/cmake",
        "CMAKE_TRY_COMPILE_TARGET_TYPE": "STATIC_LIBRARY",
        "USE_CUDNN": "ON",
        "USE_CUSPARSELT": "ON",
        "CAFFE2_USE_CUDNN": "ON"
      },
      "environment": {
        "CC": "cl",
        "CXX": "cl",
        "CUDACXX": "nvcc"
      }
    },
    {
      "name": "CUDA-Debug",
      "hidden": false,
      "inherits": [
        "CUDA"
      ],
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Debug"
      }
    },
    {
      "name": "CUDA-Release",
      "hidden": false,
      "inherits": [
        "CUDA"
      ],
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Release"
      }
    },
    {
      "name": "CUDA-Visual-Studio-Debug",
      "hidden": false,
      "inherits": [
        "CUDA-Debug"
      ],
      "generator": "Visual Studio 17 2022",
      "cacheVariables": {
        "CMAKE_GENERATOR": "Visual Studio 17 2022"
      }
    },
    {
      "name": "CUDA-Visual-Studio-Release",
      "hidden": false,
      "inherits": [
        "CUDA-Release"
      ],
      "generator": "Visual Studio 17 2022",
      "cacheVariables": {
        "CMAKE_GENERATOR": "Visual Studio 17 2022"
      }
    }
  ]
}